<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Consulta.
    -->
    <changeSet id="20181031033426-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="receita_id"
                                 baseTableName="consulta"
                                 constraintName="fk_consulta_receita_id"
                                 referencedColumnNames="id"
                                 referencedTableName="receita"/>

        <addForeignKeyConstraint baseColumnNames="consultorio_id"
                                 baseTableName="consulta"
                                 constraintName="fk_consulta_consultorio_id"
                                 referencedColumnNames="id"
                                 referencedTableName="consultorio"/>

        <addForeignKeyConstraint baseColumnNames="paciente_id"
                                 baseTableName="consulta"
                                 constraintName="fk_consulta_paciente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="paciente"/>

        <addForeignKeyConstraint baseColumnNames="medico_id"
                                 baseTableName="consulta"
                                 constraintName="fk_consulta_medico_id"
                                 referencedColumnNames="id"
                                 referencedTableName="medico"/>

    </changeSet>
</databaseChangeLog>

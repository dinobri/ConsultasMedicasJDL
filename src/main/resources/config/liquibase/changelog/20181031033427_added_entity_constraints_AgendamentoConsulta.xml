<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity AgendamentoConsulta.
    -->
    <changeSet id="20181031033427-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="consulta_id"
                                 baseTableName="agendamento_consulta"
                                 constraintName="fk_agendamento_consulta_consulta_id"
                                 referencedColumnNames="id"
                                 referencedTableName="consulta"/>

        <addForeignKeyConstraint baseColumnNames="paciente_id"
                                 baseTableName="agendamento_consulta"
                                 constraintName="fk_agendamento_consulta_paciente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="paciente"/>

        <addForeignKeyConstraint baseColumnNames="medico_id"
                                 baseTableName="agendamento_consulta"
                                 constraintName="fk_agendamento_consulta_medico_id"
                                 referencedColumnNames="id"
                                 referencedTableName="medico"/>

    </changeSet>
</databaseChangeLog>

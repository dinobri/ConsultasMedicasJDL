        <addForeignKeyConstraint baseColumnNames="receita_id"
                                 baseTableName="posologia"
                                 constraintName="fk_posologia_receita_id"
                                 referencedColumnNames="id"
                                 referencedTableName="receita"/>

    </changeSet>
</databaseChangeLog>
